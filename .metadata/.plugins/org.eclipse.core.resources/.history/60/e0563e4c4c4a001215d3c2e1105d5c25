public class HUD implements Updatable, Drawable, Disposable {

	private ShapeRenderer shape;

	public HUD() {

		shape = new ShapeRenderer();
	}

	@Override
	public void update() {
		// TODO Auto-generated method stub

	}

	@Override
	public void draw(SpriteBatch batch) {
		// TODO Auto-generated method stub

	}

	@Override
	public void dispose() {
		shape.dispose();
	}

}
